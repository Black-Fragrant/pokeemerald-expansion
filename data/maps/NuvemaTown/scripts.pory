script NuvemaTown_EventScript_OffToRoute1
{
	lock()
	setvar(VAR_NUVEMA_PICK_STARTER, 8)
	playse(SE_PIN)
	applymovement(LOCALID_NUVEMA_BIANCA_WAIT_AT_ROUTE_1, moves(emote_exclamation_mark))
	waitmovement(0)
	waitse()
	delay(20)
	playbgm(MUS_BW_ADVENTURE_TOGETHER, TRUE)
	applymovement(LOCALID_NUVEMA_BIANCA_WAIT_AT_ROUTE_1, moves(walk_in_place_down))
	waitmovement(0)
	msgbox("Bianca: {PLAYER}...\n"
    	   "It's this way!", MSGBOX_DEFAULT)
	closemessage()
	getplayerxy (VAR_TEMP_0, VAR_TEMP_1)
	call_if_eq (VAR_TEMP_0, 18, NuvemaTown_EventScript_OTR1_Pos1)
	call_if_eq (VAR_TEMP_0, 19, NuvemaTown_EventScript_OTR1_Pos2)
	call_if_eq (VAR_TEMP_0, 20, NuvemaTown_EventScript_OTR1_Pos3)
	call_if_eq (VAR_TEMP_0, 21, NuvemaTown_EventScript_OTR1_Pos4)
	call_if_eq (VAR_TEMP_0, 22, NuvemaTown_EventScript_OTR1_Pos5)
	call_if_eq (VAR_TEMP_0, 23, NuvemaTown_EventScript_OTR1_Pos6)
	delay(10)
	applymovement(LOCALID_NUVEMA_CHEREN_WAIT_AT_ROUTE_1, moves(walk_in_place_down))
	waitmovement(0)
	msgbox("Cheren: Bianca says if we're starting a\n"
			"journey together, she wants us all to\l"
			"take our first step at the same time.", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_CHEREN_WAIT_AT_ROUTE_1)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_BIANCA_WAIT_AT_ROUTE_1, moves(walk_down, walk_in_place_left))
	waitmovement(0)
	applymovement(LOCALID_PLAYER, moves(walk_in_place_right))
	waitmovement(0)
	msgbox("Bianca: Hey, {PLAYER}!\p"
			"Let's all take our first step\n"
			"on Route 1 together!", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_BIANCA_WAIT_AT_ROUTE_1)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_CHEREN_WAIT_AT_ROUTE_1, moves(walk_in_place_up))
	waitmovement(0)
	msgbox("Cheren: OK, here we go!", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_CHEREN_WAIT_AT_ROUTE_1)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_BIANCA_WAIT_AT_ROUTE_1, moves(walk_up))
	applymovement(LOCALID_PLAYER, moves(walk_up))
	waitmovement(0)
	msgbox("One, two!", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_BIANCA_WAIT_AT_ROUTE_1)
	destroy_speech_bubble()
	closemessage()
	applymovement(LOCALID_NUVEMA_BIANCA_WAIT_AT_ROUTE_1, moves(walk_up * 4))
	applymovement(LOCALID_NUVEMA_CHEREN_WAIT_AT_ROUTE_1, moves(walk_up * 4))
	applymovement(LOCALID_PLAYER, moves(walk_up * 4))
	waitmovement(0)
	setflag(FLAG_NUVEMA_CB_WAIT_AT_ROUTE_1)
	clearflag(FLAG_ROUTE1_EAST_CB_BOTTOM)
	warp(MAP_ROUTE1_EAST, 20, 58)
	waitstate()
	release()
	end()
}

