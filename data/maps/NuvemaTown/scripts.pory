script NuvemaTown_EventScript_MotherGivesTownMap
{
	lockall()
	setvar(VAR_NUVEMA_PICK_STARTER, 7)
	setflag(FLAG_NUVEMA_PJL_CHEREN_BIANCA)
	delay(10)
	playse(SE_DOOR)
	setdooropen(9, 6)
	waitse()
    delay (10)
	addobject(LOCALID_NUVEMA_BIANCA_LEAVING_LAB)
	playse(SE_WALL_HIT)
	waitse()
	delay(10)
	msgbox("Bianca: Heeey!\n"
		"{PLAYER}, wait up!", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_BIANCA_LEAVING_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_PLAYER, moves( walk_down ))
	applymovement(LOCALID_NUVEMA_BIANCA_LEAVING_LAB, moves( walk_down ))
	waitmovement(0)
	addobject(LOCALID_NUVEMA_CHEREN_LEAVING_LAB)
	applymovement(LOCALID_NUVEMA_BIANCA_LEAVING_LAB, moves( walk_right, walk_down ))
	applymovement(LOCALID_NUVEMA_CHEREN_LEAVING_LAB, moves( walk_down, walk_left , walk_down ))
	waitmovement(0)
	setdoorclosed(9, 6)
	delay(20)
	msgbox("Mom: Oh, there you are! And...?\n"
			"What did the professor have to say?\p"
			"She asked you to complete the Pokédex?!\n"
			"I can't believe it!\p"
			"Well, actually, I can.\n"
			"I already knew she was going to ask.", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_MOTHER_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_MOTHER_LAB, moves( walk_up ))
	waitmovement(0)
	msgbox("Mom: That's why I brought you three of\n"
			"these Town Maps. Take them with you!", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_MOTHER_LAB)
	destroy_speech_bubble()
	closemessage()
    playbgm (MUS_BW_OBTAIN_KEY_ITEM, TRUE)
    msgbox("{PLAYER} obtained the Town Map!")
    delay(120)
	additem (ITEM_TOWN_MAP)
    call_if_eq (VAR_RESULT, FALSE, NuvemaTown_EventScript_BagIsFull)
    closemessage
    playbgm (MUS_BW_NUVEMA_TOWN, TRUE)
	delay(10)
	applymovement(LOCALID_NUVEMA_MOTHER_LAB, moves( walk_left, walk_in_place_up ))
	waitmovement(0)
	delay(10)
	msgbox("Mom: Here you are, Cheren.", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_MOTHER_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_CHEREN_LEAVING_LAB, moves( walk_in_place_down ))
	waitmovement(0)
	msgbox("Cheren: I'll take good care of it.", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_CHEREN_LEAVING_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_MOTHER_LAB, moves( walk_right, walk_right, walk_in_place_up ))
	waitmovement(0)
	delay(10)
	msgbox("Mom: And one for you, too, Bianca.", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_MOTHER_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_BIANCA_LEAVING_LAB, moves( walk_in_place_down ))
	waitmovement(0)
	msgbox("Bianca: Th-thank you so much!", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_BIANCA_LEAVING_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	msgbox("Mom: As for your room, {PLAYER}--\n"
			"or what's left of it...\p"
			"No need for any of you to worry.\n"
			"I'll take care of tidying up.\p"
			"OK, {PLAYER}?", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_MOTHER_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_MOTHER_LAB, moves( walk_left, walk_in_place_up ))
	waitmovement(0)
	delay(10)
	msgbox("Mom: Ah, Pokémon. They are so cute,\n"
			"but they have enough power to destroy\l"
			"a bedroom! They are really something.\p"
			"With Pokémon like that at your side,\n"
			"you'll be safe wherever you go!\p"
			"I'll let your parents know!\p"
			"I hope that, in addition to Pokémon,\n"
			"you'll find lots and lots of places\l"
			"you like in the Unova region--\l"
			"and become wonderful adults!\p"
			"Have a great trip!", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_MOTHER_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(20)
	applymovement(LOCALID_NUVEMA_MOTHER_LAB, moves( walk_down * 6 ))
	waitmovement(0)
	removeobject(LOCALID_NUVEMA_MOTHER_LAB)
	setflag(FLAG_NUVEMA_MOTHER_LAB)
	delay(10)
	applymovement(LOCALID_NUVEMA_CHEREN_LEAVING_LAB, moves( walk_in_place_right ))
	waitmovement(0)
	applymovement(LOCALID_NUVEMA_BIANCA_LEAVING_LAB, moves( walk_in_place_left ))
	applymovement(LOCALID_PLAYER, moves( walk_in_place_left ))
	waitmovement(0)
	delay(10)
	msgbox("Cheren: If I use the Town Map,\n"
			"I'll always know where I am...\l"
			"That's certainly helpful.\p"
			"Shall we head to Route 1?\n"
			"The professor's waiting.", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_CHEREN_LEAVING_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_BIANCA_LEAVING_LAB, moves( walk_in_place_left ))
	waitmovement(0)
	delay(10)
	msgbox("Bianca: Let's go, let's go!\n"
			"Nate, hurry and come too, OK?", MSGBOX_DEFAULT, TRUE, LOCALID_NUVEMA_BIANCA_LEAVING_LAB)
	destroy_speech_bubble()
	closemessage()
	delay(10)
	applymovement(LOCALID_NUVEMA_BIANCA_LEAVING_LAB, moves( walk_right * 8 ))
	applymovement(LOCALID_NUVEMA_CHEREN_LEAVING_LAB, moves( walk_up, walk_right * 9 ))
	waitmovement(0)
	removeobject(LOCALID_NUVEMA_BIANCA_LEAVING_LAB)
	removeobject(LOCALID_NUVEMA_CHEREN_LEAVING_LAB)
	clearflag(FLAG_NUVEMA_CB_WAIT_AT_ROUTE_1)
	delay(20)
    releaseall()
	end()
}

